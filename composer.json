{
  "name": "paksuco/support",
  "description": "WIP - FAQ and ticket system for Laravel",
  "keywords": ["laravel", "faq", "support", "ticket"],
  "license": "MIT",
  "authors": [
    {
      "name": "Taha PAKSU",
      "email": "tpaksu@gmail.com",
      "homepage": "https://rettica.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0",
    "illuminate/config": "^7.0",
    "illuminate/routing": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Paksuco\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paksuco\\Support\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Paksuco\\Support\\SupportServiceProvider"
        ],
        "aliases": {
            "Package": "Paksuco\\Support\\Facade"
        }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
